package practice01;

public class Practice01 {
	public static void main(String[] args) {
		for(int i=1; i<=9; i++)
		{
			for(int j=1; j<=9; j++)
			{
				System.out.println(i + " * " + j + " = " + i*j);
			}
			System.out.println();
		}
		
	}

}

package practice01;

public class Practice02 {
	 public static void main(String[] args) {
		byte a=10;
		short b=20;
		int c=30;
		long d= 40;
		System.out.printf("a: %d, b: %d, c: %d, d: %d",a,b,c,d);
	}
}

